<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<!-- By default, Ehcache stored the cached files in temp folder. -->
	<!-- <diskStore path="java.io.tmpdir" /> -->

	<defaultCache name="defaut" maxElementsInMemory="500000"
		eternal="false" timeToIdleSeconds="1800" timeToLiveSeconds="3600"
		overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="OPENCQT.CMQ_PRODUCT_BASE_CURRENT" maxElementsInMemory="50000"
		eternal="false" timeToIdleSeconds="1800" timeToLiveSeconds="3600"
		overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.CmqProductBaseCurrent"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.CmqProductBaseTarget"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.CmqRelation190"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.CmqRelationTarget"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.MeddraDict190"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.MeddraDictTarget"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.ProductConfigCodeList"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" transactionalMode="off" />

	<cache name="com.dbms.entity.cqt.ProgramConfigCodeList"
		maxElementsInMemory="50000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" diskPersistent="false"
